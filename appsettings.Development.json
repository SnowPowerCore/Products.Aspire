{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ReverseProxy": {
    "Clusters": {
      "backend-products": {
        "Destinations": {
          "backend-products": {
            "Address": "https://backend-products"
          }
        }
      }
    },
    "Routes": {
      "backend-products-openapi": {
        "ClusterId": "backend-products",
        "AuthorizationPolicy": "anonymous",
        "Match": {
          "Path": "/api/products/openapi/{**catch-all}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/api/products"
          }
        ]
      },
      "backend-products-openapi-root": {
        "ClusterId": "backend-products",
        "AuthorizationPolicy": "anonymous",
        "Match": {
          "Path": "/openapi/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPrefix": "/api/products"
          }
        ]
      },
      "backend-products-scalar": {
        "ClusterId": "backend-products",
        "AuthorizationPolicy": "anonymous",
        "Match": {
          "Path": "/api/products/scalar/{**catch-all}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/api/products"
          }
        ]
      },
      "backend-products-antiforgery": {
        "ClusterId": "backend-products",
        "AuthorizationPolicy": "anonymous",
        "Match": {
          "Path": "/api/products/antiforgerytoken/{**catch-all}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/api/products"
          }
        ]
      },
      "backend-products-captchachallenge": {
        "ClusterId": "backend-products",
        "AuthorizationPolicy": "anonymous",
        "Match": {
          "Path": "/api/products/{**catch-all}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/api/products"
          }
        ]
      }
    }
  }
}